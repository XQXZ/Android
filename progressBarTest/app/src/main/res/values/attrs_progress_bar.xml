<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HorizontalProgressBarWithProgressBar">
        <attr name="progress_color_reach" format="color"></attr>
        <attr name="progress_color_unreach" format="color"></attr>
        <attr name="progress_height_reach" format="dimension"></attr>
        <attr name="progress_height_unreach" format="dimension"></attr>
        <attr name="progress_text_size" format="dimension"></attr>
        <attr name="progress_text_color" format="color"></attr>
        <attr name="progress_text_offset" format="dimension"></attr>
        <attr name="progress_text_visibility" format="enum">
            <enum name="invisiable" value="1"></enum>
            <enum name="visiable" value="0"></enum>
        </attr>
    </declare-styleable>
    <declare-styleable name="RoundProgressBarWithProgressBar" >
        <attr name="radius" format="dimension"></attr>
    </declare-styleable>
</resources>
